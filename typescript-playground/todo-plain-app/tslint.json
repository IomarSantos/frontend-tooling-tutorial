{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "member-access": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-type"
    ],
    "no-string-literal": false
  }
}